    "\n",
    "for dr in discount_rates:\n",
    "    for i, strategy in enumerate(strategies):\n",
    "        # Use base parameters but adjust discount rate\n",
    "        params = ttb_params[i].copy()\n",
    "        params['discount_rate'] = dr\n",
    "        \n",
    "        result = calculate_time_to_benefit(strategy, **params)\n",
    "        sensitivity_results.append({\n",
    "            'strategy': strategy,\n",
    "            'discount_rate': dr,\n",
    "            'total_discounted_qalys': result['total_discounted_qalys'],\n",
    "            'early_benefit_score': result['early_benefit_score']\n",
    "        })\n",
    "\n",
    "sens_df = pd.DataFrame(sensitivity_results)\n",
