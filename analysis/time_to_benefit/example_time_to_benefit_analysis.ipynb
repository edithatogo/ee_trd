    "# Define time-to-benefit analysis functions\n",
    "def calculate_time_to_benefit(\n",
    "    strategy,\n",
    "    onset_time=30,  # days to initial benefit\n",
    "    peak_time=90,   # days to peak benefit\n",
    "    duration=365,   # duration of benefit in days\n",
    "    effectiveness_decay=False,\n",
    "    baseline_utility=0.7,\n",
    "    peak_benefit_utility=0.9,\n",
    "    discount_rate=0.035  # Annual discount rate for NZ, 0.05 for AU\n",
    "):\n",
    "    \"\"\"\n",
    "    Calculate time-to-benefit profile for a treatment strategy.\n",
    "    \n",
    "    Args:\n",
    "        strategy: Treatment strategy name\n",
    "        onset_time: Time to initial benefit (days)\n",
    "        peak_time: Time to peak benefit (days)\n",
    "        duration: Duration of benefit (days)\n",
    "        effectiveness_decay: Whether effectiveness decays over time\n",
    "        baseline_utility: Utility in unhealthy state\n",
    "        peak_benefit_utility: Peak utility achieved\n",
    "        discount_rate: Annual discount rate\n",
    "        \n",
    "    Returns:\n",
    "        Dictionary with time-to-benefit analysis results\n",
    "    \"\"\"\n",
    "    # Create time series\n",
    "    time_points = np.arange(0, duration + 1, 7)  # Every 7 days\n",
    "    \n",
    "    # Calculate utility trajectory\n",
    "    utilities = []\n",
    "    for t in time_points:\n",
    "        if t < onset_time:\n",
    "            # Before onset, utility is at baseline\n",
    "            utility = baseline_utility\n",
    "        elif t < peak_time:\n",
    "            # During ramp-up phase, utility increases linearly\n",
    "            progress = (t - onset_time) / (peak_time - onset_time)\n",
    "            utility = baseline_utility + progress * (peak_benefit_utility - baseline_utility)\n",
    "        else:\n",
    "            # After peak, stay at peak level or decay if specified\n",
    "            if effectiveness_decay:\n",
    "                # Decay exponentially after peak\n",
    "                decay_factor = np.exp(-(t - peak_time) / (duration * 0.7))  # 70% of duration as time constant\n",
    "                utility = baseline_utility + decay_factor * (peak_benefit_utility - baseline_utility)\n",
    "            else:\n",
    "                # Maintain peak benefit\n",
    "                utility = peak_benefit_utility\n",
    "        \n",
    "        utilities.append(utility)\n",
    "    "\n",
    "    # Calculate discounted utility\n",
    "    discount_factors = []\n",
    "    for t in time_points:\n",
    "        years = t / 365.25\n",
    "        discount_factor = 1 / (1 + discount_rate) ** years\n",
    "        discount_factors.append(discount_factor)\n",
    "    \n",
    "    discounted_utilities = np.array(utilities) * np.array(discount_factors)\n",
    "    \n",
    "    # Calculate cumulative benefits\n",
    "    # Approximate using trapezoidal rule\n",
    "    dt = 7  # Time step in days\n",
    "    daily_discounted_utils = []\n",
    "    \n",
    "    for i in range(len(time_points)-1):\n",
    "        # Linear interpolation between time points\n",
    "        t1, t2 = time_points[i], time_points[i+1]\n",
    "        u1, u2 = discounted_utilities[i], discounted_utilities[i+1]\n",
    "        \n",
    "        # Average utility between time points\n",
    "        avg_daily_util = (u1 + u2) / 2\n",
    "        daily_discounted_utils.extend([avg_daily_util] * (t2 - t1))\n",
    "    \n",
    "    # Calculate cumulative QALYs\n",
    "    cumulative_qalys = []\n",
    "    cum_qaly = 0\n",
    "    for i, util in enumerate(daily_discounted_utils):\n",
    "        # Convert daily utility to QALY increment\n",
    "        qaly_increment = util * (1/365.25)  # Daily utility * fraction of year\n",
    "        cum_qaly += qaly_increment\n",
    "        cumulative_qalys.append(cum_qaly)\n",
    "    \n",
    "    # Calculate key metrics\n",
    "    half_peak_time = onset_time + (peak_time - onset_time) / 2\n",
    "    \n",
    "    # Time to achieve 50% of peak benefit\n",
    "    half_peak_util = baseline_utility + 0.5 * (peak_benefit_utility - baseline_utility)\n",
    "    \n",
    "    # Find when utility reaches half-peak\n",
    "    for i, t in enumerate(time_points):\n",
    "        if t >= onset_time and t <= peak_time:\n",
    "            expected_util = baseline_utility + ((t - onset_time) / (peak_time - onset_time)) * (peak_benefit_utility - baseline_utility)\n",
    "            if expected_util >= half_peak_util:\n",
    "                time_to_half_peak = t\n",
    "                break\n",
    "    else:\n",
    "        time_to_half_peak = peak_time\n",
    "    \n",
    "    return {\n",
    "        'strategy': strategy,\n",
    "        'time_series': time_points,\n",
    "        'utilities': np.array(utilities),\n",
    "        'discounted_utilities': discounted_utilities,\n",
    "        'cumulative_qalys': cumulative_qalys,\n",
    "        'baseline_utility': baseline_utility,\n",
    "        'peak_benefit_utility': peak_benefit_utility,\n",
    "        'onset_time': onset_time,\n",
    "        'peak_time': peak_time,\n",
    "        'duration': duration,\n",
    "        'effectiveness_decay': effectiveness_decay,\n",
    "        'discount_rate': discount_rate,\n",
    "        'time_to_half_peak': time_to_half_peak,\\n",
    "        'total_discounted_qalys': cumulative_qalys[-1] if cumulative_qalys else 0,\n",
    "        'early_benefit_score': sum(cumulative_qalys[:int(len(cumulative_qalys)*0.2)])  # First 20% of period\n",
    "    }"
   ]
