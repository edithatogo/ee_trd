    "# Calculate combined time-benefit index\n",
    "# This combines multiple time-to-benefit factors into a single index\n",
    "def calculate_combined_time_benefit_index(onset_time, peak_time, total_qalys, early_score, duration=365):\n",
    "    \"\"\"\n",
    "    Calculate a combined time-benefit index.\n",
    "    \n",
    "    Factors: Faster onset and peak, higher early benefit, greater total benefit\n",
    "    \"\"\"\n",
    "    # Normalize time measures (lower is better) - invert so higher is better\n",
    "    onset_score = 1 / (1 + onset_time/30)  # Higher score for faster onset\n",
    "    peak_score = 1 / (1 + peak_time/60)    # Higher score for faster peak\n",
    "    \n",
    "    # Total QALYs - higher is better\n",
    "    qaly_score = total_qalys * 10  # Scale for comparability\n",
    "    \n",
    "    # Early benefit score - higher is better\n",
    "    early_score_scaled = early_score * 1000  # Scale for comparability\n",
    "    \n",
    "    # Combined score (weighted average)\n",
    "    combined_score = (onset_score * 0.2 + peak_score * 0.2 + qaly_score * 0.3 + early_score_scaled * 0.3)\n",
    "    \n",
    "    return {\n",
    "        'onset_score': onset_score,\n",
    "        'peak_score': peak_score,\n",
    "        'qaly_score': qaly_score,\n",
    "        'early_score': early_score_scaled,\n",
    "        'combined_score': combined_score\n",
    "    }\n",
    "\n",
    "# Calculate combined indices\n",
    "combined_indices = []\n",
    "for result in ttb_results:\n",
    "    idx = calculate_combined_time_benefit_index(\n",
    "        result['onset_time'], \n",
    "        result['peak_time'], \n",
    "        result['total_discounted_qalys'], \n",
    "        result['early_benefit_score']\n",
    "    )\n",
    "    idx['strategy'] = result['strategy']\n",
    "    combined_indices.append(idx)\n",
    "\n",
    "combined_df = pd.DataFrame(combined_indices)\n",
    "print(\"COMBINED TIME-BENEFIT INDEX\")\n",
    "print(\"=\"*60)\n",
    "print(combined_df)\n",
    "\n",
    "# Visualize combined index\n",
    "fig, ax = plt.subplots(1, 2, figsize=(16, 6))\n",
    "\n",
    "# 1. Combined score by strategy\n",
    "colors = ['blue' if s == 'ECT' else 'green' if s == 'IV-KA' else 'orange' for s in combined_df['strategy']]\n",
    "ax[0].bar(combined_df['strategy'], combined_df['combined_score'], color=colors, alpha=0.7)\n",
    "ax[0].set_ylabel('Combined Time-Benefit Index')\n",
    "ax[0].set_title('Combined Time-Benefit Index by Strategy')\n",
    "ax[0].grid(True, alpha=0.3)\n",
    "\n",
    "# Add value labels\n",
    "for i, v in enumerate(combined_df['combined_score']):\n",
    "    ax[0].text(i, v + max(combined_df['combined_score'])*0.01, \n",
    "                 f'{v:.3f}', ha='center', va='bottom')\n",
    "\n",
    "# 2. Component contributions to combined score\n",
    "components = ['onset_score', 'peak_score', 'qaly_score', 'early_score']\n",
    "component_labels = ['Onset Speed', 'Peak Speed', 'Total Benefit', 'Early Benefit']\n",
    "\n",
    "bottom = np.zeros(len(combined_df))\n",
    "for i, (comp, label) in enumerate(zip(components, component_labels)):\n",
    "    values = combined_df[comp].values\n",
    "    ax[1].bar(combined_df['strategy'], values, bottom=bottom, \n",
    "              label=label, alpha=0.7, color=plt.cm.Set3(i))\n",
    "    bottom += values\n",
    "\n",
    "ax[1].set_ylabel('Score Contribution')\n",
